{
  "manifest_version": 3,
  "name": "Cookie.box",
  "version": "1.0",
  "description": "View, edit, add, and delete cookies with ease. Perfect for session token management.",
  "permissions": [
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Cookie.box"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
